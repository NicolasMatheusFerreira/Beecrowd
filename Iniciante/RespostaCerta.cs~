using System;

class RespostaCerta {
	
	static void Main(string[] args) {
		
		int qtdQuestoes;		

		qtdQuestoes = int.Parse(Console.ReadLine());

		int[] resposta = new int[qtdQuestoes];

		for(int i = 1; i<=qtdQuestoes; i++) {
			resposta[i-1] = int.Parse(Console.ReadLine());	
		}

		
	}
}
